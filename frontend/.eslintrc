{
    "extends": [
        "wesbos"
    ],
    "rules": {
        "no-console": 2,
        "no-use-before-define": 0,
        "react/prop-types": 0,
        "prettier/prettier": [
            "error",
            {
                "singleQuote": true,
                "printWidth": 120,
                "code": 80,
                "tabWidth": 2,
                "comments": 80,
                "ignoreComments": false,
                "ignoreTrailingComments": true,
                "ignoreUrls": true,
                "ignoreStrings": true,
                "ignoreTemplateLiterals": true,
                "ignoreRegExpLiterals": true,
                "trailingComma": "es5"
            }
        ]
    }
}